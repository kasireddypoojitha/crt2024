#include <stdio.h>

int main() {
    int a,b;
    scanf("%d%d",&a,&b);
    int choice;
    scanf("%d",&choice);
    switch(choice)
    {
        case 1:
               printf("%d",a+b);
               break;
        case 2:
               printf("%d",a-b);
               break;
        case 3:
               printf("%d",a*b);
               break;
        case 4:
               printf("%d",a/b);
               break;
       default:
            printf("Invalid choice");
            
    }
    // Write C code here
    printf("Try programiz.pro");
